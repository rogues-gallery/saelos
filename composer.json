{
  "name": "dbhurley/saelos",
  "description": "Saelos, the Sales CRM",
  "keywords": [
    "saelos",
    "crm"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1",
    "doctrine/dbal": "^2.6",
    "fideloper/proxy": "~3.3",
    "giggsey/libphonenumber-for-php": "^8.8",
    "laracasts/generators": "^1.1",
    "laravel/framework": "5.5.*",
    "laravel/passport": "^4.0",
    "laravel/tinker": "~1.0",
    "phpsafari/health-checks": "^0.0.7",
    "predis/predis": "^1.1",
    "processmaker/laravel-i18next": "dev-master as 1.0",
    "pusher/pusher-php-server": "^3.0@dev",
    "rachidlaasri/laravel-installer": "dev-master as 4.1",
    "rap2hpoutre/create-user-command": "^0.1.4",
    "twilio/sdk": "^5.16",
    "webklex/laravel-imap": "^1.2"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "barryvdh/laravel-ide-helper": "^2.4",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "mpociot/laravel-apidoc-generator": "dev-master",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
